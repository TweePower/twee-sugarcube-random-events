{
  "name": "twee-sugarcube-random-events",
  "version": "1.0.0",
  "description": "",
  "types": "./src/type/*",
  "scripts": {
    "build-prod": "webpack -c webpack.prod.config.js && node ./build/build.js prod",
    "build-dev": "webpack -c webpack.dev.config.js && node ./build/build.js dev",
    "build-quick-start-step-0": "tweego -o ./twee_dist/quick_start_step0.html ./twee/quickStart/step0",
    "build-quick-start-step-1": "tweego -o ./twee_dist/quick_start_step1.html ./twee/quickStart/step1 ./dist/random-event.js",
    "build-quick-start-step-2": "tweego -o ./twee_dist/quick_start_step2.html ./twee/quickStart/step2 ./dist/random-event.js",
    "build-quick-start-step-3": "tweego -o ./twee_dist/quick_start_step3.html ./twee/quickStart/step3 ./dist/random-event.js",
    "build-quick-start-step-4": "tweego -o ./twee_dist/quick_start_step4.html ./twee/quickStart/step4 ./dist/random-event.js",
    "build-quick-start-step-5": "tweego -o ./twee_dist/quick_start_step5.html ./twee/quickStart/step5 ./dist/random-event.js",
    "build-quick-start-step-6": "tweego -o ./twee_dist/quick_start_step6.html ./twee/quickStart/step6 ./dist/random-event.js",
    "build-quick-start-step-7": "tweego -o ./twee_dist/quick_start_step7.html ./twee/quickStart/step7 ./dist/random-event.js",
    "build-doc": "npm run build-quick-start-step-0 && npm run build-quick-start-step-1 && npm run build-quick-start-step-2 && npm run build-quick-start-step-3 && npm run build-quick-start-step-4 && npm run build-quick-start-step-5 && npm run build-quick-start-step-6 && npm run build-quick-start-step-7",
    "build-all": "npm run build-prod && npm run build-dev && npm run build-doc",
    "lint": "eslint src",
    "test": "jest --coverage --collectCoverageFrom='src/**/*.ts'"
  },
  "author": "tweepower@gmail.com",
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/twine-sugarcube": "^2.36.11",
    "@typescript-eslint/eslint-plugin": "^7.3.0",
    "eslint": "^8.57.0",
    "jest": "^27.0.6",
    "ts-jest": "^27.1.5",
    "ts-loader": "^9.5.1",
    "typescript": "^4.1.3",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=20.7"
  }
}
